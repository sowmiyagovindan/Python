node{
     stage('Checkout'){
        checkout scm
     }
     stage('Example Build') {
        workspace = pwd()
        echo "${workspace}"
        sh 'virtualenv env'
        sh "${workspace}/env/bin/pip install -r requirements/prod.txt
        env.PATH = "${workspace}/env/bin:/usr/bin:${env.PATH}"
        echo "${env.PATH}"
        sh 'python3 ${workspace}/HW.py'
        echo 'Your file was executed'

        }

}
