node{
    stage('Example Build') {
        agent { docker 'maven:3-alpine' }
             {
                echo 'Hello, Maven'
                sh 'mvn --version'
            }
        }
        stage('Example Test') {
            agent { docker 'openjdk:8-jre' }
             {
                echo 'Hello, JDK'
                sh 'java -version'
            }
        }

}
